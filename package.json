{
  "name": "edu-vision-dashboard",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "http-server -p 8080 dist",
    "serve:mcp": "node mcp-server/index.js",
    "local": "npm run build && VITE_SUPABASE_URL=$VITE_SUPABASE_URL VITE_SUPABASE_ANON_KEY=$VITE_SUPABASE_ANON_KEY concurrently -k \"npm:serve\" \"npm:serve:mcp\"",
    "lint": "eslint . --ext ts,tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "@tensorflow/tfjs": "^4.11.0",
    "chart.js": "^4.5.1",
    "face-api.js": "^0.22.2",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/papaparse": "^5.3.8",
    "vite": "^4.4.5",
    "typescript": "^5.2.2",
    "tailwindcss": "^3.3.5",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1"
  }
}
